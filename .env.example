# Server Configuration
PORT=3000

# Base Configuration
BASE_URL=https://animepahe.si
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36

# Cookie Configuration
# Format: cookie1=value1; cookie2=value2
COOKIES=

# Proxy Configuration
USE_PROXY=true
# Format: 
PROXIES=http://proxy1.com:8080,http://user:pass@proxy2.com:8080

# Redis Configuration
# Format: redis://default:password@host:port
# Example for Redis Cloud: redis://default:password123@redis-12345.c123.us-east-1-1.ec2.cloud.redislabs.com:12345
REDIS_URL=

# Rate Limiting Configuration (only applies when RATE_LIMIT_SECRET is set)
# Format: a random secret string (e.g., a UUID or random string) - only set this on your Vercel deployment
# Note: Rate limiting requires REDIS_URL to function properly
RATE_LIMIT_SECRET=
# Maximum requests per window
RATE_LIMIT_MAX=100
# Time window in seconds (default: 900 seconds = 15 minutes)
RATE_LIMIT_WINDOW=900 